{
  "name": "svg-music-logos",
  "version": "2.1.0",
  "description": "A collection of bands' and musicians' logos in SVG.",
  "homepage": "http://tiagoporto.github.io/svg-music-logos",
  "private": true,
  "keywords": [
    "logos",
    "music",
    "vector",
    "svg",
    "band",
    "download"
  ],
  "author": {
    "name": "Tiago Porto",
    "url": "http://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tiagoporto/svg-music-logos.git"
  },
  "scripts": {
    "start": "echo 'please run `npm run gulp` in one tab and then `npm run webpack` in another one'",
    "build": "npm run gulp:build && npm run webpack:build",
    "deploy": "npm run build && gh-pages -d dist",
    "gulp": "gulp watch",
    "gulp:build": "gulp build",
    "prewebpack": "npm run generate-data",
    "webpack": "webpack-dev-server --hot",
    "webpack:build": "webpack --mode=production --profile --json > stats.json",
    "checks": "npm run check-formatting && npm run lint",
    "stats": "npm run webpack:build && webpack-bundle-analyzer stats.json",
    "check-formatting": "prettier --check --ignore-unknown '**/*'",
    "formatting:fix": "npm run check-formatting -- --write",
    "lint": "eslint --ext js,jsx,ts,tsx,md,mdx ./src --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "update-readme": "gulp update-readme",
    "generate-data": "gulp generate-data"
  },
  "dependencies": {
    "file-saver": "^2.0.2",
    "lodash": "^4.17.19",
    "normalize.css": "^8.0.1",
    "postcss-easing-gradients": "^3.0.1",
    "svg-to-inline": "^1.0.0-alpha.1",
    "vue": "^2.6.11",
    "vue-analytics": "^5.20.1",
    "vue-flag-icon": "^1.0.6",
    "vue-router": "^3.1.3",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "del": "^5.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.0.1",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.1.1",
    "gulp": "^4.0.2",
    "gulp-concat-jsons": "git+https://github.com/totango/gulp-concat-jsons.git",
    "gulp-ignore": "^3.0.0",
    "gulp-jsonminify": "^1.1.0",
    "gulp-replace": "^1.0.0",
    "gulp-stylus": "^2.7.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^3.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.2.1",
    "style-loader": "^1.1.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-loader": "^15.8.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "husky": {
    "hooks": {
      "pre-push": "echo 'Running checks...' && npm run checks"
    }
  }
}
