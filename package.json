{
  "name": "svg-music-logos",
  "version": "0.0.0-development",
  "description": "Music-related logos and symbols collection in SVG.",
  "homepage": "https://tiagoporto.github.io/svg-music-logos/",
  "type": "module",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Tiago Porto",
    "url": "https://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagoporto/svg-music-logos.git"
  },
  "engines": {
    "node": ">=22",
    "npm": ">=10"
  },
  "scripts": {
    "prepare": "husky",
    "start": "nuxt start",
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev",
    "prebuild": "npm run generate-styles && npm run generate-data",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "gulp": "gulp watch",
    "update-readme": "gulp update-readme",
    "generate-data": "gulp generate-data",
    "generate-styles": "gulp generate-styles",
    "checks": "npm run check-formatting && npm run check-docs && npm run typecheck && npm run lint && npm run lint:styles",
    "check-formatting": "prettier --check --ignore-unknown \"**/*\"",
    "check-formatting:fix": "npm run check-formatting -- --write",
    "check-docs": "remark --frail .",
    "typecheck": "vue-tsc",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:styles": "stylelint \"**/*.{scss,vue}\"",
    "lint:styles:fix": "stylelint \"**/*.{scss,vue}\" --fix"
  },
  "dependencies": {
    "@nuxt/scripts": "^0.9.5",
    "@nuxtjs/google-fonts": "^3.2.0",
    "change-case": "^5.4.4",
    "file-saver": "^2.0.5",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.11.1",
    "prettier": "^3.4.1",
    "svg-to-inline": "1.0.0-beta.4",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.4.21",
    "vue-country-flag-next": "^2.3.2",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^0.7.2",
    "@tiagoporto/remark-config": "^1.0.0",
    "@types/file-saver": "^2.0.7",
    "@types/throttle-debounce": "^5.0.2",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint": "^9.16.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-vue": "^9.32.0",
    "gulp": "^5.0.0",
    "gulp-changed": "^5.0.2",
    "gulp-concat-json-to-array": "^0.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-stylus": "^3.0.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-html": "^1.7.0",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "stylelint": "^16.11.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.6.2",
    "vue-tsc": "^2.1.10"
  },
  "overrides": {
    "eslint-config-standard": {
      "eslint": ">=9.0.0"
    }
  }
}
