{
  "name": "svg-music-logos",
  "version": "0.0.0-development",
  "description": "Music-related logos and symbols collection in SVG.",
  "homepage": "https://svg-music-logos.tiagoporto.com/",
  "type": "module",
  "private": true,
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Tiago Porto",
    "url": "https://tiagoporto.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiagoporto/svg-music-logos.git"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "version": ">=23",
      "onFail": "error"
    },
    "packageManager": {
      "name": "npm",
      "version": ">=10",
      "onFail": "error"
    }
  },
  "packageManager": "npm@10.9.2",
  "scripts": {
    "postinstall": "$CI = true || (dependabot-config && editor-config)",
    "new-artist": "node scripts/new-artist.mjs",
    "new-logo": "node scripts/new-logo.mjs",
    "predev": "npm run prebuild",
    "dev": "open http://localhost:3000 && concurrently -k \"gulp watch\" \"nuxt dev\"",
    "prebuild": "gulp build",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preupdate-readme": "npm run generate-data",
    "update-readme": "gulp update-readme",
    "copy-svgs": "gulp copy-svgs",
    "generate-data": "gulp generate-data",
    "generate-styles": "gulp generate-styles",
    "check-update": "npx npm-check --update",
    "checks": "npm run check-docs && npm run format:check && npm run typecheck && npm run lint && npm run lint:styles",
    "check-docs": "remark --frail .",
    "format:check": "prettier --check --ignore-unknown \"**/*\"",
    "format": "npm run format:check -- --write",
    "typecheck": "npx nuxi typecheck",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "lint:styles": "stylelint \"**/*.{scss,vue}\"",
    "lint:styles:fix": "stylelint \"**/*.{scss,vue}\" --fix",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:3000 'playwright test'",
    "test:e2e:dev": "start-server-and-test dev http-get://localhost:3000 'playwright test --ui'"
  },
  "dependencies": {
    "nuxt": "^4.2.1"
  },
  "devDependencies": {
    "@codecov/nuxt-plugin": "^1.9.1",
    "@commitlint/cli": "^19.6.0",
    "@dzangolab/vue-country-flag-icon": "^0.3.0",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/image": "^1.11.0",
    "@nuxt/scripts": "^0.12.1",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@playwright/test": "^1.56.1",
    "@sentry/nuxt": "^10.27.0",
    "@tiagoporto/dependabot-config": "^2.5.0",
    "@tiagoporto/editor-config": "^2.0.0",
    "@tiagoporto/eslint-config": "^6.1.0",
    "@tiagoporto/prettier-config": "^2.1.0",
    "@tiagoporto/remark-config": "^1.11.0",
    "@tiagoporto/stylelint-config": "^1.2.0",
    "@types/gulp": "^4.0.17",
    "@types/node": "^22.14.1",
    "@types/throttle-debounce": "^5.0.2",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.4.0",
    "@vue/test-utils": "^2.4.6",
    "change-case": "^5.4.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.1",
    "eslint-plugin-playwright": "^2.3.0",
    "eslint-plugin-vue": "^10.5.1",
    "globals": "^16.4.0",
    "gulp": "^5.0.0",
    "gulp-changed": "^5.0.4",
    "gulp-concat-json-to-array": "^0.0.1",
    "gulp-replace": "^1.1.4",
    "gulp-sass": "^6.0.0",
    "gulp-zip": "^6.1.0",
    "happo-e2e": "^5.0.0",
    "happo-playwright": "^4.0.3",
    "happo.io": "^12.0.2",
    "inquirer": "^12.9.4",
    "lefthook": "^2.0.2",
    "nuxt-gtag": "^4.0.0",
    "postcss-html": "^1.8.0",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.6.2",
    "remark": "^15.0.1",
    "remark-cli": "^12.0.1",
    "sass-embedded": "^1.92.1",
    "start-server-and-test": "^2.0.11",
    "stylelint": "^16.24.0",
    "stylelint-config-recommended-scss": "^16.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "throttle-debounce": "^5.0.2",
    "through2": "^4.0.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.22",
    "vue-router": "^4.5.0",
    "vue-tsc": "^3.1.2",
    "vuetify-nuxt-module": "^0.18.7"
  },
  "overrides": {
    "@dzangolab/vue-country-flag-icon": {
      "vue": ">=3.5.13"
    },
    "@vercel/nft": "^0.27.4",
    "@codecov/nuxt-plugin": {
      "nuxt": ">=4.0.0"
    }
  }
}
